	@echo "compiling minishell..."
	
	@make all -sC ./libft
	@$(CC) $(CFLAGS) $(OBJ) -I $(LIB) $(LIBFT) -o $(NAME) $(LDFLAGS)
	@echo "minishell compiled!!"
	@echo	"\033[0m"

clean:
	@echo "cleaning everything..."
	@rm -f $(OBJ)
	@make clean -sC ./libft

fclean: clean
	@rm -f $(NAME)
	@make fclean -sC ./libft

re: fclean all

.PHONY: all clean fclean re
